<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<BorderPane prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hanzereversiai.projectp3.ui.OnlineLobbySubscriptionEntry">
   <center>
      <Label fx:id="subscriptionStatusLabel" text="Unsubscribed" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <Label fx:id="gameNameLabel" text="Board game name" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets left="10.0" />
         </BorderPane.margin>
      </Label>
   </left>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <right>
      <Button fx:id="subUnsubButton" mnemonicParsing="false" onAction="#onSubUnsubButtonActivated" text="Subscribe" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
